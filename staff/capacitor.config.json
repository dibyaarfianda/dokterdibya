{
  "appId": "com.dokterdibya.staff",
  "appName": "Dibya Klinik Staff",
  "webDir": "public",
  "server": {
    "url": "https://dokterdibya.com/staff/public/install.html",
    "cleartext": false,
    "androidScheme": "https"
  },
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 2000,
      "backgroundColor": "#1d2e5b",
      "androidSplashResourceName": "splash",
      "showSpinner": false
    }
  },
  "android": {
    "allowMixedContent": true,
    "captureInput": true,
    "webContentsDebuggingEnabled": true,
    "buildOptions": {
      "keystorePath": "",
      "keystorePassword": "",
      "keystoreAlias": "",
      "keystoreAliasPassword": "",
      "releaseType": "APK"
    }
  }
}
